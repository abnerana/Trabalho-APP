<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.cotemig.abner.financas">

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_icone"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".InserirDespesa"
            android:label="@string/str_tela_inserir"
            />
        <activity
            android:name=".VerDespesas"
            android:label="@string/str_tela_rel"
           />

        <activity
            android:name=".PreferenceActivity"
            android:label="@string/str_tela_opcoes">

        <intent-filer>
            <action android:name=".PreferenceActivity" />
            <category android:name="android.intent.category.PREFERENCE" />
        </intent-filer>

        </activity>



    </application>

</manifest>
